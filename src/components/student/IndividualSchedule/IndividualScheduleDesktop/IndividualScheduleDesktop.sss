.root
  display: flex
  flex-direction: row
  justify-content: space-between
  flex-wrap: wrap
  margin-top: 30px

.dayGroup
  display: flex
  flex-direction: column
  width: 185px

.dayGroupName
  font-size: 14px
  line-height: 24px
